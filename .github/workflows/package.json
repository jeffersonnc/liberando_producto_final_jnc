{
    "name": "test-repo-multi-semantic-release",
    "private": true,
    "workspaces": [
      "src/"
    ],
    "dependencies": {
      "@qiwi/multi-semantic-release": "^6.5.1"
    },
    "release": {
      "branches": [
        "main"
      ],
      "plugins": [
        "@semantic-release/commit-analyzer",
        "@semantic-release/release-notes-generator",
        "@semantic-release/github"
      ]
    },
    "multi-release": {
      "tagFormat": "${name}-v${version}"
    }
  }
